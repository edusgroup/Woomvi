{% spaceless %}
<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8">
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />

    <meta property="og:type"        content="article" />
    <meta property="og:url"         content="" />
    <meta property="og:title"       content="{{ seo.title }}" />
    <meta property="og:image"       content="" />
    <meta property="og:description" content="{{ seo.description }}" />

    <link rel="canonical" href="http://wumvi.com/"/>
    <meta itemprop="url" href="http://wumvi.com/"/>

    <meta name="description" content="{{ seo.description }}"/>
    <title>{{ seo.title }}</title>

    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <meta name="google-site-verification" content="{{ google.verification }}" />
    <meta name='yandex-verification' content="{{ yandex.verification }}" />
    <meta name='wmail-verification' content="" />

    <meta http-equiv="cleartype" content="on">

    <link rel="shortcut icon" href="/favicon.ico" />
    <link href="/res/icons/icon128.png" rel="icon"/>
    <link href="/res/icons/icon128.png" rel="apple-touch-icon-precomposed"/>

    <!-- Responsive and mobile friendly stuff -->
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <link rel="stylesheet"  href="/res/css/base.css">
    <link rel="stylesheet"  href="/res/css/responsive.gs.24col.css">

    <link href="/res/sass/bin/index.css" rel="stylesheet"/>

    <script src="/res/js/lang/ru.js"></script>

    <!--<link href='http://fonts.googleapis.com/css?family=Open+Sans:400,700,600&subset=latin,cyrillic-ext' rel='stylesheet' type='text/css'> -->
    <!-- <link href="/res/css/font-awesome-4.3.0/css/font-awesome.min.css" rel="stylesheet"/> -->


    <script>
        var wumvi = {
            AJAX_SUCCESS: 1,
            AJAX_FAIL: 0,

            storage: {
                user: {}
            }
        };

        wumvi.storage.user.isAuth = {{ isAuth }};
    </script>
	
	{% block head %}{% endblock %}


</head>
<body>

{# include("global/menu/main.twig") #}

<header class="menu-view" id="menu-view">
    <div class="headline">
        <div class="container">
            <ul class="list">
                <li>
                    <a href="">Магазин</a>
                </li>
                <li class="login">
                    {% if isAuth %}
                        <a href="{{ route('user.kabinet') }}">Личный кабинет</a>
                        /
                        <a href="{{ route('user.exit') }}">Выйти</a>
                    {% else %}
                        <a href="{{ route('user.login') }}">Войти</a>
                        /
                        <a href="{{ route('user.registration') }}">Зарегистрироваться</a>
                    {% endif %}
                </li>
            </ul>
        </div>
    </div>
    <div class="menu-box small">
        <div class="container">
            <a href="/" class="logo"><img src="/res/img/logo.png"/></a>
            <nav class="row">
                <a href="{{ route('course') }}" class="menu-link-nav">Программа изучения</a>
                <a href="{{ route('main.article', 'samostoyatelno') }}" class="menu-link-nav">Самостоятельно</a>
                <a href="{{ route('main.article', 'v-grupe') }}" class="menu-link-nav">В группе</a>
                <a href="{{ route('main.article', 'v-grupe') }}" class="menu-link-nav">Помощь</a>
                <a href="{{ route('main.article', 'v-grupe') }}" class="menu-link-nav">Общение</a>
            </nav>
        </div>
    </div>

</header>


{% block content %}{% endblock %}

<div id="footercontainer">
    <footer class="group">
        <a href="/english/">Блог английского языка</a>
    </footer>
</div>

<script src="/res/js/respond.min.js"></script>
<script src="/res/bower_package/jquery/dist/jquery.min.js"></script>
<!-- <script src="http://yastatic.net/jquery/cookie/1.0/jquery.cookie.min.js"></script> -->

<script src="/res/js/main.js"></script>

{% block footer %}{% endblock %}
</body>
</html>
{% endspaceless %}